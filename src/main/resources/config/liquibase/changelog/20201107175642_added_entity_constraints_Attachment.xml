<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Attachment.
    -->
    <changeSet id="20201107175642-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="attachment_file_id"
                                 baseTableName="attachment"
                                 constraintName="fk_attachment_attachment_file_id"
                                 referencedColumnNames="id"
                                 referencedTableName="attachment_file"/>

        <addForeignKeyConstraint baseColumnNames="remark_id"
                                 baseTableName="attachment"
                                 constraintName="fk_attachment_remark_id"
                                 referencedColumnNames="id"
                                 referencedTableName="remark"/>

    </changeSet>
</databaseChangeLog>
