<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Patient.
    -->
    <changeSet id="20200922185327-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="location_id"
                                 baseTableName="patient"
                                 constraintName="fk_patient_location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location"/>

        <addForeignKeyConstraint baseColumnNames="insurer_id"
                                 baseTableName="patient"
                                 constraintName="fk_patient_insurer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="insurer"/>

    </changeSet>
</databaseChangeLog>
